config {
	doctype = html5
	htmlTag_setParams = lang="de" dir="ltr"
	htmlTag_langKey = de
	locale_all = de_DE
	language = de
	noScaleUp = 1
	removeDefaultCss = 1
	removePageCss = 1
	simulateStaticDocuments = 0
	spamProtectEmailAddresses_atSubst = (at)
	sys_language_uid = 0
	absRefPrefix = http://{$domain}/
	baseURL = http://{$domain}/
	tx_realurl_enable = 1
	
	compressCss = 1
	compressJs = 1
	concatenateJsAndCss = 1
}